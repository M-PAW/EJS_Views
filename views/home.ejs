<%- include("partials/header") -%>

<h1>Favorite User Quotes</h1>
<p><%= homeContent %></p>
<% posts.forEach(post => { %>
<div class="card">
    <h3>"<%=post.title%>"</h3>
    <h5>~<em><%=post.author%></em></h5>
    <p><%= post.content.substring(0,115)+"..."%> 
        <a href="/posts/<%=post.title%>">Read More</a> 
    </p>
</div>
<%})  %>

<%- include("partials/footer") -%>